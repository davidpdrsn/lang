fn main() {
    fizz_buzz(1);
}

fn fizz_buzz(n: Integer) {
    if n <= 30 {
        if n % 15 == 0 {
            println("Fizz Buzz");
        } else {
            if n % 3 == 0 {
                println("Fizz");
            } else {
                if n % 5 == 0 {
                    println("Buzz");
                } else {
                    println(n);
                }
            }
        }

        fizz_buzz(n + 1);
    }
}
